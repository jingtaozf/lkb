not-allowed :=
%prefix (a *)
[ CATEG cat1,
  ARGS < [ CATEG cat0 ] > ].

not-allowed1 :=
%prefix
[ CATEG cat1,
  ARGS < [ CATEG cat0 ] > ].

not-allowed2 :=
%prifix (* a)
[ CATEG cat1,
  ARGS < [ CATEG cat0 ] > ].

not-allowed3 :=
%prefix (* ?a)
[ CATEG cat1,
  ARGS < [ CATEG cat0 ] > ].
