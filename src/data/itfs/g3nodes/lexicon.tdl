[ ORTH "the",
  CATEG det ].

sleep := pl-word &
[ ORTH "sleep",
  CATEG vp ].

sleeps := sg-word &
[ ORTH "sleeps",
  CATEG vp ].

dog := sg-word &
[ ORTH "dog",
  CATEG n ].

dogs := pl-word &
[ ORTH "dogs",
  CATEG n ].

