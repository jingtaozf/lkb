% EXAMPLE MACROS
%
% Counters
\newcounter{examplectr}%[chapter]
\newcounter{antsubexamplectr}%[chapter]
\newcounter{figexamplectr}%[chapter]
%
% Example Macro
\newenvironment{ex}%
   {\vspace{.1in}\addtocounter{examplectr}{1}
     \setcounter{antsubexamplectr}{0}
     \begin{list}
       {(\arabic{examplectr})}%
       {\setlength{\topsep}{0in}
       	\setlength{\leftmargin}{.25in}
	       \setlength{\labelsep}{0.125in}}
       \item \begin{minipage}[t]{5.75in} % was 4.25
   }%
   {\end{minipage}
    \end{list}\vspace{.2in}}
% changed from .1 to .2 for dspacing
%
% Sub-example Macro
\newenvironment{subex}%
   { \addtocounter{antsubexamplectr}{1}
     \begin{list}
       {\alph{antsubexamplectr}}%
       {\setlength{\topsep}{-\parskip}
	       \setlength{\leftmargin}{0.175in}
	       \setlength{\labelsep}{0.125in}}
       \item
   }%
   {\end{list}}
%
\newenvironment{figex}%
   {\addtocounter{figexamplectr}{1}
    \begin{center}
}%
   {\ \	\\[2mm] Figure \arabic{figexamplectr}\end{center}}
%
% Reference to examples and figures by relative offsets:
\newcommand{\exnum}[2]{\addtocounter{examplectr}{#1}(\arabic{examplectr}{#2})\addtocounter{examplectr}{-#1}}
%
\newcommand{\fignum}[1]{\addtocounter{figexamplectr}{#1}Figure \arabic{figexamplectr}\addtocounter{figexamplectr}{-#1}} 


\newcommand{\dictdef}[2]{
{\bf #1} #2 \\}

